{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "importsNotUsedAsValues": "error",
    "moduleResolution": "bundler",
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "src/**/*.d.ts"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}

